				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="`pkg-config gtk+-2.0 --cflags`" />
			<Add option="-Wall" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config gtk+-2.0 --libs`" />
		</Linker>
		<Unit filename="Geometric.cpp" />
		<Unit filename="Geometric.h" />
		<Unit filename="data_structure.cpp" />
		<Unit filename="data_structure.h" />
